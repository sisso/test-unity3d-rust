enum MessageKind : ushort {
	StartGame = 0,
	CreateObj,
	MoveObj,
	SetInputAxis,
}

struct IdPackage {
    id: uint32;
}

struct PosPackage {
   x: float32;
   y: float32;
}

table StringPackage {
   buffer: string;
}

table BytesPackage {
   buffer: [byte];
}

table Package {
   kind: MessageKind;
   id: IdPackage;
   pos: PosPackage;
   str: StringPackage;
   bytes: BytesPackage;
}

root_type Package;